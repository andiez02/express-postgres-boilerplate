{
  "name": "project-name-api",
  "version": "1.0.0",
  "description": "Project Name API",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon",
    "lint": "eslint --ext=ts src",
    "lint:fix": "eslint --fix --quiet --ext=ts src",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "db:build:migrations": "tsc -p tsconfig.migrations.json",
    "db:migration:generate": "sequelize migration:generate --migrations-path src/migrations --name",
    "db:migrate": "yarn db:build:migrations && sequelize db:migrate",
    "db:migrate:undo": "yarn db:build:migrations && sequelize db:migrate:undo",
    "db:seed:generate": "sequelize seed:generate --seeders-path src/seeders --name",
    "db:seed": "npm run db:build:migrations && sequelize db:seed:all",
    "clean": "rm -rf dist",
    "build": "tsc"
  },
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@elastic/ecs-winston-format": "^1.3.1",
    "axios": "^0.21.1",
    "axios-auth-refresh": "^3.1.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.20.0",
    "chalk": "^4.1.2",
    "class-validator": "^0.12.2",
    "compression": "1.7.1",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dayjs": "^1.11.3",
    "dotenv": "^8.2.0",
    "email-templates": "^8.0.3",
    "express": "^4.17.1",
    "express-validation": "^3.0.5",
    "helmet": "^4.3.1",
    "http-status": "^1.6.2",
    "json2csv": "^5.0.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.1",
    "pg": "^8.3.0",
    "pug": "^3.0.0",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "sequelize": "6.6.5",
    "sequelize-cli": "^6.2.0",
    "stripe": "^9.11.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/email-templates": "^8.0.4",
    "@types/express": "^4.17.7",
    "@types/jest": "^28.1.3",
    "@types/lodash": "^4.14.161",
    "@types/node": "12.0.2",
    "@types/node-cron": "^2.0.3",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.9",
    "@types/pg": "^7.14.9",
    "@types/redis": "^2.8.28",
    "@types/request": "^2.48.5",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "concurrently": "^5.3.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.7",
    "jest": "^28.1.1",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run prettier",
      "npm run lint:fix",
      "git add ."
    ]
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "license": "UNLICENSED"
}
